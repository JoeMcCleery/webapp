{
  "name": "@webapp/orm-old",
  "version": "1.0.0",
  "dependencies": {
    "@prisma/client": "~6.18.0",
    "@zenstackhq/runtime": "^2.22.2"
  },
  "devDependencies": {
    "@trpc/server": "^11.8.1",
    "@webapp/auth-utils": "workspace:*",
    "@webapp/ts-config": "workspace:*",
    "@zenstackhq/trpc": "^2.22.2",
    "prisma": "~6.18.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zenstack": "^2.22.2",
    "zod": "^4.3.5"
  },
  "files": [
    "src"
  ],
  "main": "src/index.ts",
  "packageManager": "pnpm@10.27.0",
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed.ts"
  },
  "private": true,
  "scripts": {
    "db:deploy": "prisma migrate deploy",
    "db:generate": "zenstack generate --output src/generated/zenstack",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio --port 3000 --browser none",
    "generate": "pnpm db:generate"
  },
  "zenstack": {
    "schema": "./src/schema.zmodel"
  }
}
