{
  "name": "@webapp/orm",
  "version": "1.0.0",
  "dependencies": {
    "@prisma/client": "~6.8.0",
    "@zenstackhq/runtime": "^2.15.0"
  },
  "devDependencies": {
    "@trpc/server": "^11.1.4",
    "@webapp/auth-utils": "workspace:*",
    "@webapp/ts-config": "workspace:*",
    "@zenstackhq/trpc": "^2.15.0",
    "prisma": "~6.8.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "zenstack": "^2.15.0",
    "zod": "^3.25.36"
  },
  "files": [
    "src"
  ],
  "main": "src/index.ts",
  "packageManager": "pnpm@10.11.0",
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed.ts"
  },
  "private": true,
  "scripts": {
    "db:deploy": "prisma migrate deploy",
    "db:generate": "zenstack generate --output src/generated/zenstack",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio --port 3000 --browser none",
    "generate": "pnpm db:generate"
  },
  "zenstack": {
    "schema": "./src/schema.zmodel"
  }
}
